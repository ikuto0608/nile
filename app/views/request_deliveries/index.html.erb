<p id="notice"><%= notice %></p>

<h1>Request Deliveries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_deliveries.each do |request_delivery| %>
      <tr>
        <td><%= link_to 'Show', request_delivery %></td>
        <td><%= link_to 'Edit', edit_request_delivery_path(request_delivery) %></td>
        <td><%= link_to 'Destroy', request_delivery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request Delivery', new_request_delivery_path %>
