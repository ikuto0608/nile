<p id="notice"><%= notice %></p>

<h1>Offer Deliveries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offer_deliveries.each do |offer_delivery| %>
      <tr>
        <td><%= link_to 'Show', offer_delivery %></td>
        <td><%= link_to 'Edit', edit_offer_delivery_path(offer_delivery) %></td>
        <td><%= link_to 'Destroy', offer_delivery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

